	measure 'Categoria MÃ¡s Vendida' =
			
			VAR TopProduct =
			    TOPN(
			        1,
			        SUMMARIZE(
			            fact_ventas,
			            dim_producto[categoria],
			            "TotalMonto", SUM(fact_ventas[monto])
			        ),
			        [TotalMonto], DESC
			    )
